					<property name="name" value="qianer" />
				</bean>
				<bean class="com.winterframework.firefrog.game.web.bet.entity.LotteryGameGroup">
					<property name="code" value="16" />
					<property name="title" value="一星" />
					<property name="name" value="yixing" />

function  checkTime(time) {
if(time <10){
return "0" + time;
}
return ""+time;
}


function getTimerString(listitem,time,number,interval) {
var not0 = !!time,

h = Math.floor(time / 3600),
m = Math.floor((time %= 3600) / 60),
s = time % 60;
if (not0){

listitem.ele.find(".hour").html(checkTime(h));
listitem.ele.find(".minute").html(checkTime(m));
listitem.ele.find(".second").html(checkTime(s));
listitem.ele.find(".cut-off").css('display','block');
listitem.ele.find(".updateing").css('display','none');
listitem.ele.find(".issue").html(number);

}
else {
setTimeout(_addTimer(listitem.ele,listitem.szname),3000);
clearInterval(interval);
interval = 0;

listitem.ele.find(".cut-off").css('display','none');
listitem.ele.find(".updateing").css('display','block');
}
}


function go(number,list,inter) {
// console.log("go"+list.length + ","+inter);
for (var i = 0; i < list.length; i++) {
getTimerString(list[i],list[i].time ? list[i].time -= 1 : 0,number,inter);

}
}

var _go = function(num,list,inter) {
return function () {
go(num,list,inter);
}
};

var _addTimer = function (cn,szn) {
return function () {
addTimer(cn,szn);
}
}


